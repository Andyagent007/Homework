(define (f-recursive n)
  (if (< n 3)
      n
      (+ (f-recursive (- n 1)) (* 2 (f-recursive (- n 2))) (* 3 (f-recursive (- n 3))))))

(define (f-iterative n)
  (perform-iterations 1 1 n))

(define (perform-iterations a b counter)
  (if (= counter 0)
      b
      (perform-iterations (+ (- a 1) (*(- a 2) 2) (* (- a 3) 3)) a (- counter 1))))

(f-recursive 3)
(f-iterative 1)
